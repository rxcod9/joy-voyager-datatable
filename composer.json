{
    "name": "joy/voyager-datatable",
    "description": "Joy VoyagerDatatable",
    "keywords": [
        "joy",
        "voyager-datatable",
        "cms",
        "voyager",
        "laravel"
    ],
    "homepage": "https://github.com/rxcod9/joy-voyager-datatable",
    "license": "MIT",
    "authors": [{
        "name": "Ramakant Gangwar",
        "email": "gangwar.ramakant@gmail.com",
        "homepage": "https://github.com/rxcod9",
        "role": "Tech Lead"
    }],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "ext-zip": "*",
        "doctrine/dbal": "^2.5.2",
        "friendsofphp/php-cs-fixer": "^3.0",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Joy\\VoyagerDatatable\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit"
    },
    "autoload-dev": {
        "psr-4": {
            "Joy\\VoyagerDatatable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joy\\VoyagerDatatable\\VoyagerDatatableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
